{
  "name": "react-map-children",
  "version": "0.1.0",
  "description": "Map React component's children recursively",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "jest-cli": "^20.0.4",
    "react": "^15.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "scripts": {
    "build": "rollup -c -w",
    "test": "jest"
  },
  "keywords": [
    "react",
    "map",
    "children",
    "iterate",
    "recursively"
  ],
  "author": "allx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udivankin/react-map-children.git"
  },
  "bugs": {
    "url": "https://github.com/udivankin/react-map-children/issues"
  },
  "homepage": "https://github.com/udivankin/react-map-children#readme",
  "jest": {
    "cache": false,
    "roots": [
      "<rootDir>/dist",
      "<rootDir>/tests"
    ],
    "modulePaths": [
      "<rootDir>/node_modules",
      "<rootDir>/dist",
      "<rootDir>/tests"
    ],
    "testRegex": "/tests/.*\\.test\\.js$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "babel-preset-react": "^6.24.1"
  }
}
